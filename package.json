{
  "name": "boilerplate-react-native-with-expo",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "repository": "https://github.com/tientran0019/boilerplate-react-native-with-expo",
  "description": "A boilerplate for a react-native app using expo",
  "author": "Tien Tran <tientran0019@gmail.com>",
  "keywords": [
    "expo",
    "boilerplate",
    "react-native",
    "redux",
    "redux-devtools-extension"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "expo start",
    "start": "NODE_ENV=production expo start",
    "android": "expo start --android",
    "android:prod": "NODE_ENV=production expo start --android",
    "ios": "expo start --ios",
    "ios:prod": "NODE_ENV=production expo start --ios",
    "web": "expo start --web",
    "web:prod": "NODE_ENV=production expo start --web",
    "eject": "expo eject",
    "clean-install": "rm -rf node_modules && npm cache clean --force && watchman watch-del-all && yarn",
    "debug": "REACT_DEBUGGER=\"unset ELECTRON_RUN_AS_NODE && open -g 'rndebugger://set-debugger-loc?port=19001' ||\" yarn start",
    "reset": "expo r -c",
    "build:ios": "NODE_ENV=production expo build:ios --release-channel production",
    "build:android": "NODE_ENV=production expo build:android --release-channel production",
    "push": "expo publish",
    "push:prod": "NODE_ENV=production expo publish --release-channel production"
  },
  "dependencies": {
    "@expo/samples": "~3.0.3",
    "@expo/vector-icons": "^12.0.0",
    "@react-native-community/datetimepicker": "^3.4.3",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/netinfo": "^6.0.0",
    "@react-native-community/slider": "^3.0.3",
    "@react-navigation/bottom-tabs": "^5.11.9",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.4",
    "color": "^3.1.3",
    "dayjs": "^1.10.4",
    "expo": "~40.0.1",
    "expo-asset": "~8.2.2",
    "expo-constants": "~10.0.1",
    "expo-font": "~8.4.0",
    "expo-linking": "~2.1.1",
    "expo-splash-screen": "~0.9.0",
    "expo-status-bar": "~1.0.3",
    "expo-web-browser": "~9.0.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "query-string": "^7.0.0",
    "rc-field-form": "^1.20.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
    "react-native-gesture-handler": "~1.10.3",
    "react-native-safe-area-context": "3.1.9",
    "react-native-screens": "^3.1.0",
    "react-redux": "^7.2.3",
    "react-use": "^17.2.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "rn-pdf-reader-js": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "~7.13.14",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0"
  },
  "private": false
}
