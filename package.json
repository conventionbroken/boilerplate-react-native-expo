{
  "name": "react-native-boilerplate-with-expo",
  "version": "0.0.1",
  "main": "node_modules/expo/AppEntry.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tientran0019/react-native-boilerplate-with-expo"
  },
  "description": "A boilerplate for a react-native app using expo",
  "author": "Trần Đức Tiến <tientran0019@gmail.com>",
  "keywords": [
    "expo",
    "boilerplate",
    "react-native",
    "redux",
    "redux-saga",
    "redux-devtools-extension"
  ],
  "license": "MIT",
  "scripts": {
    "start": "expo start",
    "debug": "REACT_DEBUGGER=\"unset ELECTRON_RUN_AS_NODE && open -g 'rndebugger://set-debugger-loc?port=19001' ||\" yarn start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "clean-install": "rm -rf node_modules && npm cache clean --force && watchman watch-del-all && yarn",
    "eject": "expo eject"
  },
  "dependencies": {
    "expo": "^32.0.0",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prop-types": "^15.6.2",
    "rc-form": "^2.4.1",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-native-dotenv": "^0.2.0",
    "react-native-elements": "^1.0.0",
    "react-navigation": "^3.0.9",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-persist-transform-immutable": "^5.0.0",
    "redux-saga": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.1.3",
    "babel-preset-expo": "^5.0.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-babel-module": "^5.0.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4"
  }
}
